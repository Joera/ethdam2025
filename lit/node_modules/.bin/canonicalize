#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joera/Documents/ethdam2025/lit/node_modules/canonicalize/bin/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/canonicalize/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules:/home/joera/Documents/ethdam2025/node_modules:/home/joera/Documents/node_modules:/home/joera/node_modules:/home/node_modules:/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joera/Documents/ethdam2025/lit/node_modules/canonicalize/bin/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/canonicalize/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules:/home/joera/Documents/ethdam2025/node_modules:/home/joera/Documents/node_modules:/home/joera/node_modules:/home/node_modules:/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../canonicalize/bin/canonicalize.js" "$@"
else
  exec node  "$basedir/../canonicalize/bin/canonicalize.js" "$@"
fi
