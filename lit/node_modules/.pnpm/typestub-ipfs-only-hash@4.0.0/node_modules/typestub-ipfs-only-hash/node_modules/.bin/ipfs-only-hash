#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/ipfs-only-hash@4.0.0/node_modules/ipfs-only-hash/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/ipfs-only-hash@4.0.0/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/ipfs-only-hash@4.0.0/node_modules/ipfs-only-hash/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/ipfs-only-hash@4.0.0/node_modules:/home/joera/Documents/ethdam2025/lit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ipfs-only-hash@4.0.0/node_modules/ipfs-only-hash/cli.js" "$@"
else
  exec node  "$basedir/../../../../../ipfs-only-hash@4.0.0/node_modules/ipfs-only-hash/cli.js" "$@"
fi
