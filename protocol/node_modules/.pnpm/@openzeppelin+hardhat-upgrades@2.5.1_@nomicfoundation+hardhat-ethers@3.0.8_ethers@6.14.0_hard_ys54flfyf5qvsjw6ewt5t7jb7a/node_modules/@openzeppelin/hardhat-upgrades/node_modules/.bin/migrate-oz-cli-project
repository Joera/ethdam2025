else
  export NODE_PATH="/home/joera/Documents/ethdam2025/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.8_ethers@6.14.0_hard_ys54flfyf5qvsjw6ewt5t7jb7a/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/home/joera/Documents/ethdam2025/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.8_ethers@6.14.0_hard_ys54flfyf5qvsjw6ewt5t7jb7a/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/home/joera/Documents/ethdam2025/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.8_ethers@6.14.0_hard_ys54flfyf5qvsjw6ewt5t7jb7a/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/home/joera/Documents/ethdam2025/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.8_ethers@6.14.0_hard_ys54flfyf5qvsjw6ewt5t7jb7a/node_modules/@openzeppelin/node_modules:/home/joera/Documents/ethdam2025/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.8_ethers@6.14.0_hard_ys54flfyf5qvsjw6ewt5t7jb7a/node_modules:/home/joera/Documents/ethdam2025/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
