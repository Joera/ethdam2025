#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules/hardhat/internal/cli/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules/hardhat/internal/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules/hardhat/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules/hardhat/internal/cli/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules/hardhat/internal/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules/hardhat/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
fi
