#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules/solidity-coverage/plugins/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules/solidity-coverage/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules/solidity-coverage/plugins/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules/solidity-coverage/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules:/home/joera/Documents/ethdam2025/custom-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../solidity-coverage@0.8.16_hardhat@2.24.0_ts-node@10.9.2_@types+node@20.17.46_typescript@5.8.3__typescript@5.8.3_/node_modules/solidity-coverage/plugins/bin.js" "$@"
fi
